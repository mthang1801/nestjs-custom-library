{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/rmq-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/rmq-service/tsconfig.app.json",
    "assets": [
      {
        "include": "../views",
        "watchAssets": true
      },
      {
        "include": "i18n/**/*",
        "watchAssets": true
      },
      {
        "include": "**/**/*.proto",
        "watchAssets": true
      }
    ],
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "dtoFileNameSuffix": [
            ".dto.ts",
            ".entity.ts",
            ".model.ts",
            ".schema.ts"
          ]
        }
      }
    ]
  },
  "monorepo": true,
  "root": "apps/rmq-service",
  "projects": {
    "rmq-service": {
      "type": "application",
      "root": "apps/rmq-service",
      "entryFile": "main",
      "sourceRoot": "apps/rmq-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rmq-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "@app/common",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "redis-service": {
      "type": "application",
      "root": "apps/redis-service",
      "entryFile": "main",
      "sourceRoot": "apps/redis-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/redis-service/tsconfig.app.json"
      }
    },
    "send-files": {
      "type": "application",
      "root": "apps/send-files",
      "entryFile": "main",
      "sourceRoot": "apps/send-files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/send-files/tsconfig.app.json"
      }
    },
    "serializer": {
      "type": "application",
      "root": "apps/serializer",
      "entryFile": "main",
      "sourceRoot": "apps/serializer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/serializer/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "mongodb-service": {
      "type": "application",
      "root": "apps/mongodb-service",
      "entryFile": "main",
      "sourceRoot": "apps/mongodb-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mongodb-service/tsconfig.app.json"
      }
    },
    "health": {
      "type": "application",
      "root": "apps/health",
      "entryFile": "main",
      "sourceRoot": "apps/health/src",
      "compilerOptions": {
        "tsConfigPath": "apps/health/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    },
    "swagger": {
      "type": "application",
      "root": "apps/swagger",
      "entryFile": "main",
      "sourceRoot": "apps/swagger/src",
      "compilerOptions": {
        "tsConfigPath": "apps/swagger/tsconfig.app.json"
      }
    },
    "scheduler": {
      "type": "application",
      "root": "apps/scheduler",
      "entryFile": "main",
      "sourceRoot": "apps/scheduler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scheduler/tsconfig.app.json"
      }
    },
    "event": {
      "type": "application",
      "root": "apps/event",
      "entryFile": "main",
      "sourceRoot": "apps/event/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event/tsconfig.app.json"
      }
    },
    "sse": {
      "type": "application",
      "root": "apps/sse",
      "entryFile": "main",
      "sourceRoot": "apps/sse/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sse/tsconfig.app.json"
      }
    },
    "cloud-service": {
      "type": "application",
      "root": "apps/cloud-service",
      "entryFile": "main",
      "sourceRoot": "apps/cloud-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cloud-service/tsconfig.app.json"
      }
    },
    "web-sockets": {
      "type": "application",
      "root": "apps/web-sockets",
      "entryFile": "main",
      "sourceRoot": "apps/web-sockets/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web-sockets/tsconfig.app.json"
      }
    }
  }
}