{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/rmq-service/src",
	"compilerOptions": {
		"deleteOutDir": true,
		"webpack": true,
		"tsConfigPath": "apps/rmq-service/tsconfig.app.json"
	},
	"monorepo": true,
	"root": "apps/rmq-service",
	"projects": {
		"rmq-service": {
			"type": "application",
			"root": "apps/rmq-service",
			"entryFile": "main",
			"sourceRoot": "apps/rmq-service/src",
			"compilerOptions": {
				"tsConfigPath": "apps/rmq-service/tsconfig.app.json"
			}
		},
		"common": {
			"type": "library",
			"root": "libs/common",
			"entryFile": "index",
			"sourceRoot": "libs/common/src",
			"compilerOptions": {
				"tsConfigPath": "libs/common/tsconfig.lib.json"
			}
		},
		"redis-service": {
			"type": "application",
			"root": "apps/redis-service",
			"entryFile": "main",
			"sourceRoot": "apps/redis-service/src",
			"compilerOptions": {
				"tsConfigPath": "apps/redis-service/tsconfig.app.json"
			}
		}
	}
}
